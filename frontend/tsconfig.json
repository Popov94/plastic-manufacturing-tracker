{
    "compilerOptions": {
      "target": "ES6",
      "module": "ESNext",
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "jsx": "react"
    },
    "include": ["src"],
    "exclude": ["node_modules"]
  }
  